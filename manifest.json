{
  "manifest_version": 3,
  "name": "[DNO_IT] VK Copy Post",
  "version": "1.3",
  "description": "Копирует пост ВК в HTML по клику для встраивания на типовой школьный сайт",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Скопировать пост",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}